/****** Object:  StoredProcedure [dbo].[SP_GetCitiesByDepartment]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetCitiesByDepartment]
@pidDepartment int = NULL
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

   
   
   
	    SELECT 
		qq.idCity
	   ,qq.City
	   ,dp.idDepartment
	   ,dp.nameDepartment
		FROM [dbo].[TB_City] as qq
		inner Join [dbo].[TB_Department] AS dp ON dp.idDepartment = qq.idDepartment
		WHERE dp.idDepartment = @pidDepartment


		/*
		SELECT * FROM TB_Users
		SELECT * FROM TB_platform
		SELECT * FROM [dbo].[TB_Department]
		SELECT * FROM [dbo].[TB_City]
		*/
		end
	
	    




GO
