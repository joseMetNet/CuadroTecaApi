-- =============================================
CREATE PROCEDURE [dbo].[SP_GetStore]
@pidStore int = 0
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

   
   
    if(@pidStore > 0)
	begin
	 
	 
		SELECT [idStore]
			  ,[NameStore]
			  ,[country]
			  ,[priceProduct]
			  ,[PriceShipping]
		FROM [dbo].[TB_Store]
		WHERE idStore = @pidStore
		/*
		SELECT * FROM TB_Users
		SELECT * FROM TB_platform
		*/
		end
	else
	begin
	 
		SELECT [idStore]
			  ,[NameStore]
			  ,[country]
			  ,[priceProduct]
			  ,[PriceShipping]
		FROM [dbo].[TB_Store]

	end
END

