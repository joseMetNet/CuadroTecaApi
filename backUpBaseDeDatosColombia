/****** Object:  StoredProcedure [dbo].[SP_UpdateOrder]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_UpdateOrder]
@pidOrder int = null,
@pStatusEmailSale varchar(MAX) = null,
@pStatusEmailShipping varchar(MAX) = null,
@pStatusEmailDelivery varchar(MAX) = null,
@pStatusPoll varchar(MAX) = null,
@pinvoice varchar(MAX) = null,
@pSurveyResult varchar(MAX) = null




AS
BEGIN


              UPDATE [dbo].[TB_Order]
			  SET  [StatusEmailSale] = isnull(@pStatusEmailSale,[StatusEmailSale])
				  ,[StatusEmailShipping] = isnull(@pStatusEmailShipping,[StatusEmailShipping])
				  ,[StatusEmailDelivery] = isnull(@pStatusEmailDelivery,StatusEmailDelivery)
				  ,[StatusPoll] = isnull(@pStatusPoll,[StatusPoll])
			
				  ,[surveyResult] = isnull(@pSurveyResult,[surveyResult])

			WHERE idOrder = @pidOrder



			SELECT * FROM TB_StatusOrder



END
GO
/****** Object:  StoredProcedure [dbo].[SP_UpdateOrderProduction]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_UpdateOrderProduction]
@pidOrder varchar(MAX) = null,
@pinvoice varchar(MAX) = null,
@pidStatus int = NULL,
@pidUser int = NULL,
@pConveyor varchar(MAX) = null,
@pGuide varchar(MAX) = null,
@pActualDeliveryDate varchar(MAX) = null







AS
BEGIN
     
	     
		  IF (@pidStatus = 4)
		     BEGIN
				  UPDATE [dbo].[TB_Order]
				  SET  
				  [invoice] = isnull(@pinvoice,[invoice]),
				  [DateStatus4] = (dateadd(hour,(-5),getdate())),
				  [idUser] = @pidUser,
				  [idStatusOrder] = @pidStatus

			  WHERE idOrder = @pidOrder

			  END
			  ELSE IF (@pidStatus = 5)
			  BEGIN 

			      UPDATE [dbo].[TB_Order]
			      SET  
				  [DateStatus5] = (dateadd(hour,(-5),getdate())),
				  [idStatusOrder] = @pidStatus,
				  [idUser] = @pidUser


				  WHERE idOrder = @pidOrder

			  END
			  ELSE IF (@pidStatus = 6)
			    BEGIN

			      UPDATE [dbo].[TB_Order]
				  SET
				  [DateStatus6] = (dateadd(hour,(-5),getdate())),
				  [conveyor] = isnull(@pConveyor,conveyor),
				  [guide] = isnull(@pGuide,guide),
				  [idStatusOrder] = @pidStatus,
				  [StatusEmailShipping] = 'Enviado',
				  [StatusEmailDelivery] = 'Pendiente'



				  WHERE idOrder = @pidOrder

			 END
		     ELSE IF (@pidStatus = 7)
		     BEGIN

			    UPDATE [dbo].[TB_Order]
			    SET
                [DateStatus7] = (dateadd(hour,(-5),getdate())),
				[idStatusOrder] = @pidStatus,
				[ActualDeliveryDate] = @pActualDeliveryDate,
				[StatusEmailDelivery] = 'Enviado'

				  
				WHERE idOrder = @pidOrder

			 END
			 

			 --SELECT * FROM TB_Order
			 --SELECT * FROM TB_StatusOrder



END
GO
/****** Object:  StoredProcedure [dbo].[SP_UpdatePasswordUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_UpdatePasswordUser]
@pEmail varchar(max) = '',
@pPassword varbinary(max) = null,
@pKey varbinary(max) = null,
@pIv varbinary(max) = null
AS
BEGIN



		IF EXISTS(SELECT * FROM TB_Users WHERE email = @pEmail)
					BEGIN
					
									--UPDATE [dbo].[TB_Users] 
									--set 
									--[password] = isnull(@pPassword,[Password])
								 --  ,[pKey] = isnull(@pKey,pKey)
								 --  ,[pIv] = isnull(@pIv,pIv)

								 --  	WHERE email = @pEmail

								    select 
									     'Los datos del correo son :' AS api_dice
										 ,[idUser]
										 ,[firstName]
									     ,[lastName]
									     ,[idTypeUser]
									     ,[email]
									     ,idStatusUser
											 from TB_Users where email = @pEmail
											 END
											 ELSE
											 BEGIN
											 SELECT 'no existe un usuario con este correo' AS api_dice
											  ,0 AS [idUser]
										 ,'No existe' AS [firstName]
									     ,'No existe' AS [lastName]
									     ,'No existe' AS [idTypeUser]
									     ,'No existe' AS [email]
									     ,'No existe' AS idStatusUser
											 END
		/*
		SELECT * FROM TB_Users
		*/

END

GO
