/****** Object:  StoredProcedure [dbo].[sp_dropdiagram]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

	CREATE PROCEDURE [dbo].[sp_dropdiagram]
	(
		@diagramname 	sysname,
		@owner_id	int	= null
	)
	WITH EXECUTE AS 'dbo'
	AS
	BEGIN
		set nocount on
		declare @theId 			int
		declare @IsDbo 			int
		
		declare @UIDFound 		int
		declare @DiagId			int
	
		if(@diagramname is null)
		begin
			RAISERROR ('Invalid value', 16, 1);
			return -1
		end
	
		EXECUTE AS CALLER;
		select @theId = DATABASE_PRINCIPAL_ID();
		select @IsDbo = IS_MEMBER(N'db_owner'); 
		if(@owner_id is null)
			select @owner_id = @theId;
		REVERT; 
		
		select @DiagId = diagram_id, @UIDFound = principal_id from dbo.sysdiagrams where principal_id = @owner_id and name = @diagramname 
		if(@DiagId IS NULL or (@IsDbo = 0 and @UIDFound <> @theId))
		begin
			RAISERROR ('Diagram does not exist or you do not have permission.', 16, 1)
			return -3
		end
	
		delete from dbo.sysdiagrams where diagram_id = @DiagId;
	
		return 0;
	END
	
GO
/****** Object:  StoredProcedure [dbo].[SP_emailTracing]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_emailTracing]


AS
BEGIN

    DECLARE @vdate date = NULL 
    
	SET @vdate = (dateadd(hour,(-5),getdate()))

	SELECT sh.email AS emailBuyer,sh.Name + ' ' + sh.lastName AS NameBuyer ,sd.email AS emailAddress, sd.Name + ' ' + sd.lastName AS NameAddress  , qq.idOrder,isnull(qq.present,'false') AS present FROM TB_Order AS qq 
	LEFT JOIN TB_ShippingUser AS sh ON sh.idShippingUser = qq.idShippingUser
	LEFT JOIN TB_ShippingAddress AS sd ON sd.idShippingUser = sh.idShippingUser
	WHERE idStatusOrder = 7 AND DATEADD(DAY,5,DateStatus7) =  @vdate

	--SELECT * FROM TB_Order
	--UPDATE TB_Order SET DateStatus7 = '2022-03-17'
	--WHERE idOrder = 456

	


END


GO
