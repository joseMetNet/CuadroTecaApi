-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_getTotalVisitsWeb]
@pDate1 varchar(MAX) = NULL,
@pDate2 varchar(MAX) = NULL
AS
BEGIN


       SELECT COUNT(idVisitsWeb) AS TotalVisitsWeb FROM [dbo].[TB_VisitsWeb]

       WHERE  [DateAdd] >= ISNULL(@pDate1,[DateAdd]) AND  [DateAdd] <= ISNULL(@pDate2,[DateAdd])

END
