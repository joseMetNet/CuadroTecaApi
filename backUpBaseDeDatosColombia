GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetPicture]
@pidpicture int = 0
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

    if(@pidpicture > 0)
	begin
		
			SELECT [idpicture]
				  ,[uuid]
				  ,[url]
				  ,[name]
				  ,[price]
				  ,[frame]
				  ,[edge]
				  ,[filter]
			  FROM [dbo].[TB_Picture] AS qq
			  WHERE idpicture = @pidpicture
			
	end
	else
	begin
		

			SELECT [idpicture]
				  ,[uuid]
				  ,[url]
				  ,[name]
				  ,[price]
				  ,[frame]
				  ,[edge]
				  ,[filter]
			  FROM [dbo].[TB_Picture]

	end

	
		/*
		SELECT * FROM TB_Users
		*/

END

