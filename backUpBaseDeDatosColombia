/****** Object:  StoredProcedure [dbo].[SP_GetGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetGiftCard]
@pidGiftCard int = 0
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

    if(@pidGiftCard > 0)
	begin
			
			SELECT [idGiftCard]
				  ,[NameGiftCard]
				  ,[reference]
				  ,[value]
				  ,[idStatusGiftCard]
				  ,(SELECT nameStatus FROM TB_StatusGiftCard as vv WHERE vv.idStatusGiftCard = qq.idStatusGiftCard) AS Status
				  ,[dateAdd]
				  ,ISNULL([IVA],0)	 AS  IVA
			  FROM [dbo].[TB_GiftCard] AS qq
			  WHERE idGiftCard = @pidGiftCard
			
	end
	else
	begin
			
			SELECT [idGiftCard]
				  ,[NameGiftCard]
				  ,[reference]
				  ,[value]
				  ,[idStatusGiftCard]
				  ,(SELECT nameStatus FROM TB_StatusGiftCard as vv WHERE vv.idStatusGiftCard = qq.idStatusGiftCard) Status
				  ,[dateAdd]
				  ,isnull([IVA],0) AS IVA
			  FROM [dbo].[TB_GiftCard] AS qq

	end

	
		/*
		SELECT * FROM TB_Users
		*/

END

GO
