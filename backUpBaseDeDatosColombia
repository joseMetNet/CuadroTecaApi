/****** Object:  StoredProcedure [dbo].[SP_GetSalesDashboard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetSalesDashboard]
@pDate1 varchar(MAX) = NULL,
@pDate2 varchar(MAX) = NULL

AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;


         IF (@pDate1 != '1000-00-00')
		   BEGIN
		    IF(@pDate2 != '1000-00-00')
	    		  BEGIN

						 SELECT 
						 (SELECT COUNT(idOrder) FROM TB_Order WHERE [dateAdd] = convert(varchar(10), (dateadd(hour,(-5),getdate())), 120) ) AS salesToday,
						 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 4  AND [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) AS SalesInproduction,
						 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 6  AND [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) AS SalesDispatched,
						 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 7  AND [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) AS SalesDelivered,
						 (SELECT COUNT(idOrder) FROM TB_Order WHERE  [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) AS TotalsOrders,
						 (SELECT SUM(unitPrice) FROM TB_HistoOrder AS qq LEFT JOIN TB_Order AS yu ON qq.idOrder = yu.idOrder  WHERE  [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2 ) AS SalesTotals,
						 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order  WHERE  [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) Time_Dispatched_production,
						 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order  WHERE  [dateAdd] >= @pDate1  AND [dateAdd] <= @pDate2) Time_Dispatched_average

		
		       END
			   ELSE
               BEGIN
			    SELECT
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE [dateAdd] = convert(varchar(10), (dateadd(hour,(-5),getdate())), 120) ) AS salesToday,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 4 AND [dateAdd] >= @pDate1 ) AS SalesInproduction,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 6 AND [dateAdd] >= @pDate1 ) AS SalesDispatched,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 7 AND [dateAdd] >= @pDate1 ) AS SalesDelivered,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE [dateAdd] >= @pDate1  ) AS TotalsOrders,
					 (SELECT SUM(unitPrice) FROM TB_HistoOrder AS qq LEFT JOIN TB_Order AS yu ON qq.idOrder = yu.idOrder WHERE [dateAdd] >= @pDate1) AS SalesTotals,
					 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order WHERE [dateAdd] >= @pDate1) Time_Dispatched_production,
					 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order WHERE [dateAdd] >= @pDate1) Time_Dispatched_average

			   END 
		END
		ELSE IF (@pDate2 != '1000-00-00')
		BEGIN

			   SELECT 
				     (SELECT COUNT(idOrder) FROM TB_Order WHERE [dateAdd] = convert(varchar(10), (dateadd(hour,(-5),getdate())), 120) ) AS salesToday,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 4 AND [dateAdd] <= @pDate2 ) AS SalesInproduction,
				     (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 6 AND [dateAdd] <= @pDate2 ) AS SalesDispatched,
					 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 7 AND [dateAdd] <= @pDate2 ) AS SalesDelivered,
				     (SELECT COUNT(idOrder) FROM TB_Order WHERE  [dateAdd] <= @pDate2  ) AS TotalsOrders,
				     (SELECT SUM(unitPrice) FROM TB_HistoOrder AS qq LEFT JOIN TB_Order AS yu ON qq.idOrder = yu.idOrder WHERE [dateAdd] <= @pDate2 ) AS SalesTotals,
					 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order WHERE [dateAdd] <= @pDate2) Time_Dispatched_production,
				     (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order WHERE [dateAdd] <= @pDate2) Time_Dispatched_average


          
		END
		ELSE 
		BEGIN
		           SELECT
							 (SELECT COUNT(idOrder) FROM TB_Order WHERE [dateAdd] = convert(varchar(10), (dateadd(hour,(-5),getdate())), 120) ) AS salesToday,
							 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 4  ) AS SalesInproduction,
							 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 6  ) AS SalesDispatched,
							 (SELECT COUNT(idOrder) FROM TB_Order WHERE idStatusOrder = 7  ) AS SalesDelivered,
							 (SELECT COUNT(idOrder) FROM TB_Order ) AS TotalsOrders,
							 (SELECT SUM(unitPrice) FROM TB_HistoOrder AS qq LEFT JOIN TB_Order AS yu ON qq.idOrder = yu.idOrder) AS SalesTotals,
							 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order ) Time_Dispatched_production,
							 (SELECT CONVERT(time,(MAX(DateStatus4) - MIN(DateStatus6))) FROM TB_Order ) Time_Dispatched_average

							 
		END

END

--SELECT * FROM TB_Order
GO
/****** Object:  StoredProcedure [dbo].[SP_GetSalesGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetSalesGiftCard]
@pidProduct int = 0
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;


		/*
		SELECT * FROM TB_Users
		*/



		--,CONVERT(varchar(10),qq.idOrder) +


		SELECT qq.idOrder,qq.idGiftCard,gi.[value],qq.[dateAdd],qq.invoice,qq.idStatusPayOrder,isnull((SELECT aa.Name FROM TB_StatusPayOrder AS aa WHERE aa.idStatusPayOrder = qq.idStatusPayOrder),'Abortado') AS NameStatus

		,us.[Name] + ' ' + us.LastName AS FullNameBuyer

		,vv.[Name] + ' ' + vv.LastName AS FullNameAddressee,us.Email AS EamilBuyer, vv.email AS EmailAddressee,

		--gi.DateExpiration , DATEADD(HOUR, 72, qq.[dateAdd] ) AS currentDate
		DATEADD(YEAR, 1, qq.[dateAdd] ) AS DateExpiration
		,
		qq.[dateAdd]  AS currentDate
		
		,qq.StatusPoll,qq.surveyResult,qq.StatusEmailSale,isnull(qq.UsedGiftCard,'Sin usar') UsedGiftCard,qq.idPromotion,(SELECT wa.NamePromotion  FROM TB_promotion AS wa WHERE wa.idPromotion = qq.idPromotion) AS NamePromotion

		,idStatusOrder,(SELECT ede.Name FROM TB_StatusOrder AS ede WHERE ede.idStatusOrder = qq.idStatusOrder) AS NameStatusOrder,
		
		--CASE WHEN qq.present = 'false' THEN 'Tradicional' WHEN qq.present = 'true' THEN
		
		'Regalo'  AS typeSale


		FROM TB_Order AS qq

		LEFT JOIN TB_ShippingUser AS us ON us.idShippingUser = qq.idShippingUser

		LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard

		LEFT JOIN TB_ShippingAddress AS vv ON vv.idShippingUser = qq.idShippingUser

		WHERE qq.idGiftCard > 0

		ORDER BY qq.idOrder DESC

		




		--SELECT * FROM TB_Order
		--SELECT * FROM TB_ShippingUser
		-- Sumar 1 hora a la fecha y hora actual

--[dateAdd] 




		--Los datos que no tengo aun son la factura, estado 



		--SELECT * FROM TB_StatusPayOrder
		--SELECT * FROM TB_ShippingUser
		--SELECT * FROM TB_ShippingAddress
		--SELECT * FROM TB_HistoOrder
		--SELECT  * FROM TB_Order
		--SELECT  * FROM TB_GiftCard


	

		
END

GO
/****** Object:  StoredProcedure [dbo].[SP_GetSalesPerDay]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetSalesPerDay]
@pDate1 varchar(MAX) = NULL,
@pDate2 varchar(MAX) = NULL





AS
BEGIN

 

		   IF (@pDate1 != '1000-00-00')
		   BEGIN
		    IF(@pDate2 != '1000-00-00')
	    		  BEGIN
											 SELECT SUM(unitPrice) AS quantity,qq.idOrder,qq.[dateAdd]

											 FROM TB_Order AS qq
											 LEFT JOIN TB_HistoOrder AS hs On hs.idOrder = qq.idOrder

											 WHERE   [DateAdd] >= @pDate1
							                 AND     [DateAdd] <= @pDate2

											 GROUP BY qq.idOrder,qq.[dateAdd]
		       END
			   ELSE
               BEGIN
								   
								 SELECT SUM(unitPrice) AS quantity,qq.idOrder,qq.[dateAdd]

								 FROM TB_Order AS qq
								 LEFT JOIN TB_HistoOrder AS hs On hs.idOrder = qq.idOrder

								 WHERE   [DateAdd] >= @pDate1

								 GROUP BY qq.idOrder,qq.[dateAdd]
				

				END 
		    END


		ELSE IF (@pDate2 != '1000-00-00')
		BEGIN

				   
								 SELECT SUM(unitPrice) AS quantity,qq.idOrder,qq.[dateAdd]

								 FROM TB_Order AS qq
								 LEFT JOIN TB_HistoOrder AS hs On hs.idOrder = qq.idOrder

								 WHERE   [DateAdd] <= @pDate2

								 GROUP BY qq.idOrder,qq.[dateAdd]
          
		END
		ELSE 
		BEGIN
		         
								 SELECT SUM(unitPrice) AS quantity,qq.idOrder,qq.[dateAdd]

								 FROM TB_Order AS qq
								 LEFT JOIN TB_HistoOrder AS hs On hs.idOrder = qq.idOrder

								 GROUP BY qq.idOrder,qq.[dateAdd]

		END
				
END
GO
/****** Object:  StoredProcedure [dbo].[SP_GetShippingUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetShippingUser]
@pidShippingUser int = null
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

			
				SELECT 
				  idShippingUser				
			      ,[Name]
				  ,lastName
				  ,typeHome
				  ,[address]
				  ,idCity
				  ,(SELECT cy.City FROM TB_City AS cy WHERE cy.idCity = qq.idCity) as City
				  ,idDepartment
				  ,(SELECT de.nameDepartment FROM [dbo].[TB_Department] AS de WHERE de.idDepartment = qq.idDepartment) as nameDepartment
				  ,phone
				  ,[datetime]
				  ,isnull(idProduct,0) AS idProduct
				  ,qq.email
				  ,qq.Nit

				  FROM [TB_ShippingUser] as qq
			       where @pidShippingUser is null or qq.idShippingUser = @pidShippingUser
		  

	
	--SELECT * FROM [dbo].[TB_ShippingUser]
	--SELECT * FROM [dbo].[TB_ShippingAddress]
	--SELECT * FROM [dbo].TB_City
	--SELECT * FROM [dbo].[TB_Department]

	SELECT * FROM [dbo].TB_City WHERE City LIKE '%Manizales%'



END

