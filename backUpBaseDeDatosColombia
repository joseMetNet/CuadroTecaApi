-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateShippingUser]
@pidShippingUser int = 0,
@pName varchar(max) = '',
@pLastName varchar(max) = '',
@pTypeHome varchar(max) = '',
@pAddress varchar(max) = '',
@pidCity varchar(max) = '',
@pidDepartment varchar(max) = '',
@pPhone varchar(max) = '',
@pEmail varchar(max) = '',
@pidProduct int = 0,
@pidGiftCard int = 0,
@pNit varchar(max) = ''




AS
BEGIN
declare @intos table (id int);
declare @vidShippingUser int;
--declare @vidCity int;
--declare @vidDepartment int;


			--declare @d table (id int, name varchar(max));
			--insert @d EXEC	[dbo].SP_storeCities @pNameCities = @pNameCity
			--set @vidCity = (select id from @d)
		
			--declare @g table (id int, name varchar(max));
			--insert @g EXEC	[dbo].SP_storeDepartments @pNameDepartments = @pNameDepartment
			--set @vidDepartment = (select id from @g)

       if(@pidShippingUser > 0)
		begin
			UPDATE [dbo].[TB_ShippingUser]
			  SET  [Name] = isnull(@pName,[Name])
				  ,[lastName] = isnull(@pLastName,[lastName])
				  ,[typeHome] = isnull(@pTypeHome,typeHome)
				  ,[address] = isnull(@pAddress,[address])
				  ,[idCity] = isnull(@pidCity,[idCity])
				  ,[idDepartment] = isnull(@pidDepartment,idDepartment)
				  ,[phone] = isnull(@pPhone,[phone])
				  ,[email] = isnull(@pEmail,[email])
				  ,[idProduct] = isnull(@pidProduct,[idProduct])
				  ,[idGiftCard] = isnull(@pidGiftCard,[idGiftCard])
				  ,[Nit] = isnull(@pNit,[Nit])


				  




			 WHERE idShippingUser = @pidShippingUser

			 set @vidShippingUser = @pidShippingUser;
           select 
			
			'se actualizo' AS api_dice 
			,idShippingUser 
		    from [dbo].[TB_ShippingUser]
		    WHERE idShippingUser = @pidShippingUser

	end
	else
	begin
		

				INSERT INTO [dbo].[TB_ShippingUser]
						   ([Name]
						   ,[lastName]
						   ,[typeHome]
						   ,[address]
						   ,[idCity]
						   ,[idDepartment]
						   ,[phone]
						   ,[email]
						   ,[idProduct]
						   ,[idGiftCard]
						   ,[Nit]
						   )

					output inserted.idShippingUser into @intos

					 VALUES
						   (@pName
						   ,@pLastName
						   ,@pTypeHome
						   ,@pAddress
						   ,@pidCity
						   ,@pidDepartment
						   ,@pPhone
						   ,@pEmail
						   ,@pidProduct
						   ,@pidGiftCard
						   ,@pNit
						   )

				   
			 set @vidShippingUser = (select id from @intos);




			 --insert into [TB_MaterialCities]
				--  ([idMaterial]
				--  ,[idCity])

    --            select [value] = @vidMaterial , [value]
				--from string_split(@pNameCitys,',')
	  
	end
		select 
			 'se creo' AS api_dice 
			,idShippingUser
		from [dbo].[TB_ShippingUser]
	    where idShippingUser = @vidShippingUser;
	  
	--select 
	--		* 
	--	from [dbo].[TB_ShippingUser] where idShippingUser = @vidShippingUser;


		--SELECT * FROM TB_Profile
		--SELECT * FROM [TB_ShippingUser]
		--SELECT * FROM TB_City
		--SELECT * FROM [dbo].[TB_Department]
		--SELECT * FROM [dbo].[TB_Document]
		--SELECT * FROM  [TB_MaterialCities]



END


GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateShippingUserGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateShippingUserGiftCard]
@pidShippingUserGiftCard int = 0,
@pName varchar(max) = '',
@pLastName varchar(max) = '',
@pTypeHome varchar(max) = '',
@pAddress varchar(max) = '',
@pidCity varchar(max) = '',
@pidDepartment varchar(max) = '',
@pEmail varchar(max) = '',
@pidGiftCard int = 0





AS
BEGIN
declare @intos table (id int);
declare @vidShippingUserGiftCard int;

       if(@pidShippingUserGiftCard > 0)
		begin
			UPDATE [dbo].[TB_ShippingUserGiftCard]
			  SET  [Name] = isnull(@pName,[Name])
				  ,[lastName] = isnull(@pLastName,[lastName])
				  ,[typeHome] = isnull(@pTypeHome,typeHome)
				  ,[address] = isnull(@pAddress,[address])
				  ,[idCity] = isnull(@pidCity,[idCity])
				  ,[idDepartment] = isnull(@pidDepartment,idDepartment)
				  ,[email] = isnull(@pEmail,[email])
				  ,[idGiftCard] = isnull(@pidGiftCard,[idGiftCard])

			 WHERE idShippingUserGiftCard = @pidShippingUserGiftCard

			 set @vidShippingUserGiftCard = @pidShippingUserGiftCard;
             select 
			'se actualizo' AS api_dice 
			,idShippingUserGiftCard 
		    from [dbo].[TB_ShippingUserGiftCard]

		    WHERE idShippingUserGiftCard = @pidShippingUserGiftCard

	end
	else
	begin
		

				INSERT INTO [dbo].[TB_ShippingUserGiftCard]
						   ([Name]
						   ,[lastName]
						   ,[typeHome]
						   ,[address]
						   ,[idCity]
						   ,[idDepartment]
						   ,[email]
						   ,[idGiftCard]
						   )

					output inserted.idShippingUserGiftCard into @intos

					 VALUES
						   (@pName
						   ,@pLastName
						   ,@pTypeHome
						   ,@pAddress
						   ,@pidCity
						   ,@pidDepartment
						   ,@pEmail
						   ,@pidGiftCard
						   )

				   
			 set @vidShippingUserGiftCard = (select id from @intos);


			 --SELECT * FROM [dbo].[TB_GiftCard]

			 UPDATE [TB_GiftCard] SET idStatusSalesGiftCard = 1
			 WHERE idGiftCard = @pidGiftCard

		
	end
		select 
			 'se creo' AS api_dice 
			,idShippingUserGiftCard
		from [dbo].[TB_ShippingUserGiftCard]
	    where idShippingUserGiftCard = @vidShippingUserGiftCard;
	  
	--select 
	--		* 
	--	from [dbo].[TB_ShippingUser] where idShippingUser = @vidShippingUser;


		--SELECT * FROM TB_Profile
		--SELECT * FROM [TB_ShippingUser]
		--SELECT * FROM TB_City
		--SELECT * FROM [dbo].[TB_Department]
		--SELECT * FROM [dbo].[TB_Document]
		--SELECT * FROM  [TB_MaterialCities]



END


GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateUser]
@pidUser int = 0,
@pFirstName varchar(max) = '',
@pLastName varchar(max) = '',
@pidTypeUser int = 0,
@pEmail varchar(max) = '',
@pUserName varchar(max) = '',
@pPassword varbinary(max) = null,
@pKey varbinary(max) = null,
@pIv varbinary(max) = null
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

    if(@pidUser > 0)
	begin
			UPDATE [dbo].[TB_Users]
			   SET [firstName] = isnull(@pFirstName,[firstName])
				  ,[lastName] = isnull(@pLastName,[firstName])
				  ,[idTypeUser] = isnull(@pidTypeUser,idTypeUser)
				  ,[email] = isnull(@pEmail,[firstName])
				  ,[password] = isnull(@pPassword,[Password])
				  ,[pKey] = isnull(@pKey,pKey)
				  ,[pIv] = isnull(@pIv,pIv)
			 WHERE idUser = @pidUser

			 set @vIdUser = @pidUser;
	end
	else
	begin
		
			INSERT INTO [dbo].[TB_Users]
				   ([firstName]
				   ,[lastName]
				   ,[idTypeUser]
				   ,[email]
				   --,[userName]
				   ,[password]
				   ,[pKey]
				   ,[pIv]
				   ,idStatusUser)
				output inserted.idUser into @intos
			 VALUES
				   (@pFirstName
				   ,@pLastName
				   ,@pidTypeUser
				   ,@pEmail
				   --,@pUserName
				   ,@pPassword
				   ,@pKey
				   ,@pIv
				   ,1)
				   
			 set @vIdUser = (select id from @intos);

	end

	select 
		     [idUser]
			,[firstName]
			,[lastName]
			,[idTypeUser]
			,[email]
			--,[userName]
			,idStatusUser idStatus 
		from TB_Users where idUser = @vIdUser;

		/*
		SELECT * FROM TB_Users
		SELECT * FROM TB_platform
		*/

END

GO
