-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_StoreOrder]
@pidShippingUser int = null,
@pId varchar(max) = '',
@pidProduct varchar(max) = '',
@pTitle varchar(max) = '',
@pQuantity varchar(max) = '',
@pidcurrency varchar(max) = '',
@pUnitPrice varchar(max) = '',
@uuid varchar(max) = '',
@cdnUrl varchar(max) = '',
@frame varchar(max) = '',
@edge varchar(max) = '',
@pcountry varchar(max) = '',
@idPromotion int = null,
@pidGiftCardUsedForPurchase int = null,
@pidGiftCard int = null,
@pTotalPrice decimal(18,2) = 0,
@pEditUrl varchar(max) = '',
@pBalanceTemporary int = 0,
@codeGiftCard varchar(max) = null,
@pIva decimal(18,2) = 0,
@pPriceShipment int = 0,
@pPresent varchar(max) = ''

AS
BEGIN
declare @intos0 table (id int);
declare @vidOrder int;
declare @vValor int;
                        
						 

						INSERT INTO [dbo].[TB_Order]
								   (
								    [dateAdd],
								    [idStatusOrder],
								    [idShippingUser],
								    [IdMercadoPago],
									[idStatusPayOrder],
									[idPromotion],
									[StatusEmailSale],
									[StatusEmailShipping],
									[StatusEmailDelivery],
									[StatusPoll],
									[invoice],
									[surveyResult],
									[country],
									[idGiftCardUsedForPurchase],
									[codeGiftCardUsedForPurchase],
									[TotalPrice],
									[iva],
									[priceShipment],
									[present]
									)
								   output inserted.idOrder into @intos0
							 VALUES
								   ((dateadd(hour,(-5),getdate())),
								   5,@pidShippingUser,@pId,0,@idPromotion,'Pendiente','Pendiente','Pendiente','Pendiente','Pendiente','Pendiente',@pcountry,@pidGiftCardUsedForPurchase,@codeGiftCard,@pTotalPrice,@pIva,@pPriceShipment,@pPresent)

								   UPDATE TB_promotion SET couponAmount = couponAmount - 1
								   WHERE idPromotion = @idPromotion
								   set @vidOrder = (select id from @intos0);

								   UPDATE TB_ShippingUser SET present = @pPresent
								   WHERE idShippingUser = @pidShippingUser

					IF  (@codeGiftCard != null) 
					BEGIN
                        UPDATE TB_Order SET present = 'true'
						WHERE idOrder = @vidOrder

					END
                      

					IF EXISTS (SELECT * FROM TB_GiftCardUsedForPurchase WHERE codeGiftCard = @codeGiftCard) 
					
					BEGIN
					   UPDATE [TB_GiftCardUsedForPurchase] SET balanceTemporary = @pBalanceTemporary
					   WHERE codeGiftCard = @codeGiftCard


					   --SELECT * FROM [TB_GiftCardUsedForPurchase]
					   --SELECT * FROM [TB_Order]
					   --SELECT * FROM TB_GiftCard
					   --DELETE FROM TB_GiftCardUsedForPurchase
				        --SELECT * FROM TB_ShippingUser
					 END
					else
					 BEGIN

					    SET @vValor = (SELECT value FROM TB_GiftCard WHERE idGiftCard = @pidGiftCardUsedForPurchase)

						 INSERT INTO [dbo].[TB_GiftCardUsedForPurchase]
						(
						 [idGiftCardUsedForPurchase]
						,[idOrder]
						,[codeGiftCard]
						,[balance]
						,[balanceTemporary]
						)
					    VALUES
						( 
						  @pidGiftCardUsedForPurchase
						 ,@vidOrder
						 ,@codeGiftCard
						 ,@vValor
						 ,@pBalanceTemporary
						)

						IF(@pBalanceTemporary = 0)
						BEGIN

							 UPDATE TB_Order SET UsedGiftCard = 'Usada sin saldo'
							 WHERE idOrder = (SELECT idOrder FROM TB_Order WHERE codeGitfCard = @codeGiftCard)

						END
						ELSE
						BEGIN 

							 UPDATE TB_Order SET UsedGiftCard = 'Usada con saldo'
						     WHERE idOrder = (SELECT idOrder FROM TB_Order WHERE codeGitfCard = @codeGiftCard)
						END

					 END
					
					--SELECT * FROM [TB_GiftCardUsedForPurchase]
								 --SELECT * FROM TB_Order
								 --SELECT * FROM TB_GiftCard

                    
								  


									 declare @Product2 table(numero int not null identity(1, 1), idProduct int);
									 declare @pTitle2 table(numero int not null identity(1, 1), title varchar(max));
									 declare @Quantity2 table(numero int not null identity(1, 1), quantity int);
									 declare @pidcurrency2 table(numero int not null identity(1, 1), idcurrency varchar(max));
									 declare @punitPrice2 table(numero int not null identity(1, 1), unitPrice int);
									 declare @uuid2 table(numero int not null identity(1, 1), uuid varchar(max));
									 declare @cdnUrl2 table(numero int not null identity(1, 1), cdnUrl varchar(max));
									 declare @frame2 table(numero int not null identity(1, 1), frame varchar(max));
									 declare @edge2 table(numero int not null identity(1, 1), [edge] varchar(max));
									 declare @pidShippingUserGiftCard2 table(numero int not null identity(1, 1), idShippingUserGiftCard varchar(max));
									 declare @pEditUrl2  table(numero int not null identity(1, 1), [editUrl] varchar(max));


									 insert into @Product2 (idProduct) select value from string_split(@pidProduct, ',');
									 insert into @pTitle2 (title) select value from string_split(@pTitle, ',');
									 insert into @Quantity2 (quantity) select value from string_split(@pQuantity, ',');
									 insert into @pidcurrency2 (idcurrency) select value from string_split(@pidcurrency, ',');
									 insert into @pUnitPrice2 (unitPrice) select value from string_split(@pUnitPrice, ',');
									 insert into @uuid2 (uuid) select value from string_split(@uuid,',');
									 insert into @cdnUrl2 (cdnUrl) select value from string_split(@cdnUrl, ',');
									 insert into @frame2 (frame) select value from string_split(@frame, ',');
									 insert into @edge2 (edge) select value from string_split(@edge, ',');
									 insert into @pEditUrl2 ([editUrl]) select value from string_split(@pEditUrl, ',');

                            if (@cdnUrl != '' )
							  begin
							    
								 insert into TB_HistoOrder (idProduct,title,quantity,idcurrency,unitPrice,uuid,cdnUrl,frame,[edge],idOrder,editUrl)
								 select m.idProduct,t.title,q.quantity ,w.idcurrency,d.unitPrice,e.uuid,c.cdnUrl,v.frame,g.[edge],@vidOrder,zz.editUrl
								 from @Product2 m 
								 inner join @pTitle2 t on t.numero = m.numero
								 inner join @Quantity2 q on q.numero = t.numero
								 inner join @pidcurrency2 w on w.numero = q.numero
								 inner join @pUnitPrice2 d on d.numero = w.numero
								 inner join @uuid2 e on e.numero = d.numero
								 inner join @cdnUrl2 c on c.numero = e.numero
								 inner join @frame2 v on v.numero = c.numero
								 inner join @edge2 g on g.numero = v.numero
								 inner join @pEditUrl2 zz on zz.numero = g.numero



								 end
								 else
								 begin

								 insert into TB_HistoOrder (idProduct,title,quantity,idcurrency,unitPrice,idOrder)
								 select m.idProduct,t.title,q.quantity,w.idcurrency,d.unitPrice,@vidOrder
								 from @Product2 m
								 inner join @pTitle2 t on t.numero = m.numero
								 inner join @Quantity2 q on q.numero = t.numero
								 inner join @pidcurrency2 w on w.numero = q.numero
								 inner join @pUnitPrice2 d on d.numero = w.numero
								 end
								 --SELECT * FROM TB_Order
								 --SELECT * FROM TB_HistoOrder

								 SELECT @vidOrder AS vidOrder



				
END
GO
/****** Object:  StoredProcedure [dbo].[SP_StoreOrderGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
