-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeDepartments]
@pNameDepartments varchar(max) = ''

AS
BEGIN
declare @intos table (id int);
declare @intas table (id int);
declare @vidDepartment int;

		if(exists (select * from [dbo].[TB_Department] where upper(nameDepartment) = Upper(@pNameDepartments)))
		begin
			set @vidDepartment = (select idDepartment from [TB_Department] where upper(nameDepartment) = Upper(@pNameDepartments));
		end
		else
		begin
				INSERT INTO [dbo].[TB_Department]
					  ([nameDepartment]
					   )
					output inserted.idDepartment into @intas
				 VALUES
					   (Upper(@pNameDepartments)
					  )
			set @vidDepartment = (select id from @intas)

		end

		select idDepartment,nameDepartment from [TB_Department] where idDepartment = @vidDepartment

END
GO
