-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateShippingAddress]
@pidShippingAddress int = 0,
@pName varchar(max) = '',
@pLastName varchar(max) = '',
@pAddress varchar(max) = '',
@pidCity int = 0,
@pEmail varchar(max) = '',
@pidDepartment int = 0,
@pPhone varchar(max) = '',
@pidTypeClient int = 0,
@pNit varchar(max) = '',
@pidShippingUser int = 0


AS
BEGIN
declare @intos table (id int);
declare @vidShippingAddress int;
--declare @vidCity int;
--declare @vidDepartment int;


			--declare @d table (id int, name varchar(max));
			--insert @d EXEC	[dbo].SP_storeCities @pNameCities = @pNameCity
			--set @vidCity = (select id from @d)
		
			--declare @g table (id int, name varchar(max));
			--insert @g EXEC	[dbo].SP_storeDepartments @pNameDepartments = @pNameDepartment
			--set @vidDepartment = (select id from @g)


       if(@pidShippingAddress > 0)
		begin
			UPDATE [dbo].[TB_ShippingAddress]
			  SET  [Name] = isnull(@pName,[Name])
				  ,[lastName] = isnull(@pLastName,[lastName])
				  ,[address] = isnull(@pAddress,[address])
				  ,[idCity] = isnull(@pidCity,[idCity])
				  ,[idDepartment] = isnull(@pidDepartment,idDepartment)
				  ,[email] = isnull(@pEmail,[email])
				  ,[phone] = isnull(@pPhone,[phone])
				  ,[idTypeClient] = isnull(@pidTypeClient,[idTypeClient])
				  ,[nit] = isnull(@pNit,[nit])
				  ,[idShippingUser] = isnull(@pidShippingUser,[idShippingUser])
				  

			 WHERE idShippingAddress = @pidShippingAddress

			 set @vidShippingAddress = @pidShippingAddress;
           select 
			
			'se actualizo' AS api_dice 
			,idShippingAddress
		    from [dbo].[TB_ShippingAddress]
		    WHERE idShippingAddress = @pidShippingAddress

	end
	else
	begin
		

				INSERT INTO [dbo].[TB_ShippingAddress]
						   (
						    [Name]
						   ,[lastName]
						   ,[address]
						   ,[idCity]
						   ,[idDepartment]
						   ,[email]
						   ,[phone]
						   ,[idTypeClient]
						   ,[nit]
						   ,[idShippingUser]
						   )

					output inserted.idShippingAddress into @intos

					 VALUES
						   (@pName
						   ,@pLastName
						   ,@pAddress
						   ,@pidCity
						   ,@pidDepartment
						   ,@pEmail
						   ,@pPhone
						   ,@pidTypeClient
						   ,@pNit
						   ,@pidShippingUser
						   )

				   
			 set @vidShippingAddress = (select id from @intos);




			 --insert into [TB_MaterialCities]
				--  ([idMaterial]
				--  ,[idCity])

    --            select [value] = @vidMaterial , [value]
				--from string_split(@pNameCitys,',')
	 
	end
		  select 
			'se creo' AS api_dice 
			,idShippingAddress
		    from [dbo].[TB_ShippingAddress]
		   WHERE idShippingAddress = @vidShippingAddress;
	  
	--select 
	--		* 
	--	from [dbo].[TB_ShippingUser] where idShippingUser = @vidShippingUser;


		--SELECT * FROM [TB_ShippingAddress]
		--SELECT * FROM [TB_ShippingUser]
		--SELECT * FROM TB_City
		--SELECT * FROM [dbo].[TB_Department]
		--SELECT * FROM [dbo].[TB_Document]
		--SELECT * FROM  [TB_MaterialCities]



END


GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateShippingAddressGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateShippingAddressGiftCard]
@pidShippingAddressGiftCard int = 0,
@pName varchar(max) = '',
@pLastName varchar(max) = '',
@pEmail varchar(max) = '',
@pidShippingUserGiftCard int = 0


AS
BEGIN
declare @intos table (id int);
declare @vidShippingAddressGiftCard  int;
--declare @vidCity int;
--declare @vidDepartment int;


			--declare @d table (id int, name varchar(max));
			--insert @d EXEC	[dbo].SP_storeCities @pNameCities = @pNameCity
			--set @vidCity = (select id from @d)
		
			--declare @g table (id int, name varchar(max));
			--insert @g EXEC	[dbo].SP_storeDepartments @pNameDepartments = @pNameDepartment
			--set @vidDepartment = (select id from @g)


       if(@pidShippingAddressGiftCard > 0)
		begin
			UPDATE [dbo].[TB_ShippingAddressGiftCard]
			  SET  [Name] = isnull(@pName,[Name])
				  ,[lastName] = isnull(@pLastName,[lastName])
				  ,[email] = isnull(@pEmail,[email])
				  ,[idShippingUserGiftCard] = isnull(@pidShippingUserGiftCard,[idShippingUserGiftCard])
				  

			 WHERE idShippingUserGiftCard = @pidShippingAddressGiftCard

			 set @vidShippingAddressGiftCard = @pidShippingAddressGiftCard;


              select 
			
			'se actualizo' AS api_dice 
			,idShippingAddressGiftCard
		    from [dbo].[TB_ShippingAddressGiftCard]
		    WHERE idShippingAddressGiftCard = @pidShippingAddressGiftCard

	end
	else
	begin
		

				INSERT INTO [dbo].[TB_ShippingAddressGiftCard]
						   (
						    [Name]
						   ,[lastName]
						   ,[email]
						   ,[idShippingUserGiftCard]
						  
						   )

					output inserted.idShippingAddressGiftCard into @intos

					 VALUES
						   (@pName
						   ,@pLastName
						   ,@pEmail
						   ,@pidShippingAddressGiftCard
						   )

				   
			 set @vidShippingAddressGiftCard = (select id from @intos);




			 --insert into [TB_MaterialCities]
				--  ([idMaterial]
				--  ,[idCity])

    --            select [value] = @vidMaterial , [value]
				--from string_split(@pNameCitys,',')

			
	 
	end
			  select 
			     'se creo' AS api_dice 
			,idShippingAddressGiftCard
		    from [dbo].[TB_ShippingAddressGiftCard]
		   WHERE idShippingAddressGiftCard = @vidShippingAddressGiftCard;
	  


	  --SELECT * FROM [TB_ShippingAddressGiftCard]
	--select 
	--		* 
	--	from [dbo].[TB_ShippingUser] where idShippingUser = @vidShippingUser;


		--SELECT * FROM [TB_ShippingAddress]
		--SELECT * FROM [TB_ShippingUser]
		--SELECT * FROM TB_City
		--SELECT * FROM [dbo].[TB_Department]
		--SELECT * FROM [dbo].[TB_Document]
		--SELECT * FROM  [TB_MaterialCities]



END


GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateShippingUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
