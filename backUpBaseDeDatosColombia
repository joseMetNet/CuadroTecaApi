/****** Object:  StoredProcedure [dbo].[SP_GetOrdersManagement]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetOrdersManagement]
@pidOrder int = null
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;



     IF(@pidOrder > 0)
	   BEGIN

	    SELECT  qq.idOrder,CASE WHEN shu.idGiftCard > 0 THEN 'COC00' + CONVERT(varchar(10),qq.idOrder)  
		
		WHEN shu.idGiftCard = 0 THEN  'COC00' + CONVERT(varchar(10),qq.idOrder) END AS idTable,
		
		 SUM(unitPrice) AS priceTotality

		,tr.[dateAdd],tr.IdMercadoPago,tr.invoice,

		st.[Name],ISNULL(tr.idPromotion,0) AS idPromotion
		
		,ISNULL(pro.NamePromotion,'Ninguno') AS NamePromotion
		
		,shu.[Name] + ' ' + shu.lastName AS Fullname,shu.email

		,adr.[Name] + ' ' + adr.lastName AS FullNameBuyer

	    ,ISNULL(CASE  WHEN shu.present = 'true'  THEN 'Regalo' WHEN shu.present  = 'false' THEN 'Tradicional' WHEN shu.idGiftCard > 0  THEN 'GiftCard'  END,'N/A') AS TypeSale 

		,(SELECT TOP 1 COUNT(idProduct) FROM TB_HistoOrder where idOrder = @pidOrder AND idProduct = 1) AS numberFrames

		,(SELECT COUNT(qqq.idOrder)  FROM TB_HistoOrder AS qqq WHERE qqq.idProduct = 3 AND qqq.idOrder = qq.idOrder) AS NumberMessage,
		
		sto.idStatusOrder,sto.[Name] AS NameStatus,tr.idUser,

		(SELECT usr.firstName + ' ' + usr.lastName  FROM TB_Users AS usr WHERE usr.idUser = tr.idUser) AS NameOperator
		
		,tr.conveyor,tr.guide, DATEADD(HOUR, 48, tr.[dateAdd] ) AS MaximumDate, tr.DateStatus6 ,DATEADD(HOUR, 72, tr.[dateAdd] ) AS MaximumDateDelivery,tr.StatusEmailSale,tr.StatusEmailShipping,tr.StatusEmailDelivery,tr.StatusPoll,tr.surveyResult

		,tr.country,tr.ActualDeliveryDate,qq.cdnUrl,qq.frame,qq.[edge],adr.[address],qq.editUrl,isnull(tr.idShippingUser,0) AS idShippingUser

	
		FROM TB_HistoOrder AS qq

		LEFT JOIN TB_Order AS tr ON tr.idOrder = qq.idOrder

		LEFT JOIN TB_StatusPayOrder AS st ON st.idStatusPayOrder = tr.idStatusPayOrder

		LEFT JOIN TB_ShippingUser AS shu ON shu.idShippingUser = tr.idShippingUser

		LEFT JOIN TB_ShippingAddress AS adr ON adr.idShippingUser = shu.idShippingUser

		LEFT JOIN TB_promotion AS pro ON pro.idPromotion = tr.idPromotion

		LEFT JOIN TB_StatusOrder AS sto ON sto.idStatusOrder = tr.idStatusOrder 

	     WHERE  tr.idOrder = @pidOrder

		GROUP BY qq.idOrder,tr.[dateAdd],tr.IdMercadoPago,tr.invoice,st.[Name],tr.idPromotion

		,pro.NamePromotion,shu.[Name],shu.lastName,shu.email,adr.[Name],adr.lastName,

         shu.present,shu.idGiftCard,pro.NamePromotion,sto.idStatusOrder,sto.[Name],

		 tr.idUser,tr.conveyor,tr.guide,tr.StatusEmailSale,tr.StatusEmailShipping

		,tr.StatusEmailDelivery,tr.StatusPoll,tr.surveyResult,tr.country,tr.ActualDeliveryDate

		,qq.cdnUrl,qq.frame,qq.[edge],adr.[address],tr.DateStatus6,qq.editUrl,tr.idShippingUser



		--SELECT * FROM TB_HistoOrder WHERE idOrder = 505
		END
		ELSE 
		BEGIN

		SELECT tr.idGiftCardUsedForPurchase,(SELECT ccf.[value] FROM TB_GiftCard AS ccf WHERE ccf.idGiftCard = tr.idGiftCardUsedForPurchase ) AS ValueGiftCardUsedForPurchase, qq.idOrder,CASE WHEN shu.idGiftCard > 0 THEN 'CLG00' + CONVERT(varchar(10),qq.idOrder)  
		
		WHEN shu.idGiftCard = 0 THEN  'COC00' + CONVERT(varchar(10),qq.idOrder) END AS idTable,
		
		 --SUM(unitPrice) AS priceTotality
		 tr.TotalPrice AS priceTotality

		,tr.[dateAdd],tr.IdMercadoPago,tr.invoice,isnull(tr.idShippingUser,0) AS idShippingUser,
		
		isnull(st.[Name],'Abortado') AS [Name],ISNULL(tr.idPromotion,0) AS idPromotion
		
		,ISNULL(pro.NamePromotion,'Ninguno') AS NamePromotion
		
		,shu.[Name] + ' ' + shu.lastName AS FullNameBuyer,shu.email

		,adr.[Name] + ' ' + adr.lastName AS FullName,adr.[address] AS addressDelivery

	    ,CASE  WHEN shu.present = 'true'  THEN 'Regalo' WHEN shu.present  = 'false' THEN 'Tradicional' WHEN tr.idGiftCard > 0  THEN 'GiftCard'  END AS TypeSale 

		,(SELECT TOP 1 COUNT(ww.idProduct) FROM TB_HistoOrder AS ww where ww.idOrder = qq.idOrder AND idProduct = 1) AS numberFrames

		,(SELECT COUNT(qqq.idOrder)  FROM TB_HistoOrder AS qqq WHERE qqq.idProduct = 3 AND qqq.idOrder = qq.idOrder) AS NumberMessage,
		
		sto.idStatusOrder,sto.[Name] AS NameStatus,tr.idUser,

		(SELECT usr.firstName + ' ' + usr.lastName  FROM TB_Users AS usr WHERE usr.idUser = tr.idUser) AS NameOperator
		
		,tr.conveyor,tr.guide, DATEADD(HOUR, 24, tr.[dateAdd] ) AS MaximumDate, tr.DateStatus6 ,DATEADD(HOUR, 72, tr.[dateAdd] ) AS MaximumDateDelivery,'Enviado' AS StatusEmailSale,tr.StatusEmailShipping,tr.StatusEmailDelivery,tr.StatusPoll,tr.surveyResult

		,tr.country,tr.ActualDeliveryDate,qq.frame,shu.idCity,ci.City,tr.priceShipment,tr.iva

		FROM TB_HistoOrder AS qq

		LEFT JOIN TB_Order AS tr ON tr.idOrder = qq.idOrder

		LEFT JOIN TB_StatusPayOrder AS st ON st.idStatusPayOrder = tr.idStatusPayOrder

		LEFT JOIN TB_ShippingUser AS shu ON shu.idShippingUser = tr.idShippingUser

		LEFT JOIN TB_City AS ci ON ci.idCity = shu.idCity

		LEFT JOIN TB_ShippingAddress AS adr ON adr.idShippingUser = shu.idShippingUser

		LEFT JOIN TB_promotion AS pro ON pro.idPromotion = tr.idPromotion

		LEFT JOIN TB_StatusOrder AS sto ON sto.idStatusOrder = tr.idStatusOrder

		--WHERE tr.country = 'CO' 

		GROUP BY qq.idOrder,tr.[dateAdd],tr.IdMercadoPago,tr.invoice,st.[Name],tr.idPromotion

		,pro.NamePromotion,shu.[Name],shu.lastName,shu.email,adr.[Name],adr.lastName,

         shu.present,shu.idGiftCard,tr.idGiftCard,pro.NamePromotion,sto.idStatusOrder,sto.[Name],

		 tr.idUser,tr.conveyor,tr.guide,tr.DateStatus6,tr.StatusEmailSale,tr.StatusEmailShipping

		,tr.StatusEmailDelivery,tr.StatusPoll,tr.surveyResult,tr.country,tr.ActualDeliveryDate,qq.frame

		,shu.idCity,ci.City,adr.[address],tr.priceShipment,tr.iva,tr.TotalPrice,tr.idGiftCardUsedForPurchase,tr.idShippingUser


		ORDER BY qq.idOrder DESC

		END
		--SELECT * FROM TB_StatusPayOrder
		--SELECT * FROM TB_ShippingUser
		--SELECT * FROM TB_ShippingAddress
		--SELECT * FROM TB_ShippingAddress
		--SELECT * FROM TB_HistoOrder
		--SELECT  * FROM TB_Order
		--SELECT  * FROM TB_GiftCard
		--SELECT * FROM TB_promotion
		--SELECT * FROM TB_PromotionType

			

END

GO
/****** Object:  StoredProcedure [dbo].[SP_GetPendingToUseGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetPendingToUseGiftCard]


AS
BEGIN
     

		 select ads.email,ads.[Name] + ' '  + ads.lastName AS nombreResibe,shi.[Name] + ' ' + shi.lastName AS nombreComprador,qq.codeGitfCard,qq.idOrder,qq.UsedGiftCard from TB_Order AS qq
		 LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard
		 LEFT JOIN TB_ShippingUser AS shi ON shi.idShippingUser = qq.idShippingUser
		 LEFT JOIN TB_ShippingAddress AS ads ON ads.idShippingUser = shi.idShippingUser
	     WHERE gi.DateExpiration < (dateadd(hour,(-5),getdate())) AND qq.UsedGiftCard IS NULL 


		 

		INSERT INTO [dbo].[TB_LogTracing]
				   ([dateAdd])
			 VALUES
				   ((dateadd(hour,(-5),getdate())))




		 --select * from TB_Order WHERE [dateAdd] <= DATEADD(DAY, -1, GETDATE())
		 --select * from TB_GiftCard AS qq
         --select UsedGiftCard from TB_Order WHERE idOrder IN (500,502,506,510,521)
         --AND NOT EXISTS (SELECT * FROM TB_Order WHERE idGiftCardUsedForPurchase = qq.idGiftCard)

END
