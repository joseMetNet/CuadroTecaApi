/****** Object:  StoredProcedure [dbo].[SP_getGiftCardBycode]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_getGiftCardBycode]

@pCodeGitfCard VARCHAR(MAX) = NULL

AS
BEGIN
DECLARE @vidGif int = null                
                   

			    SET @vidGif = (SELECT gi.idGiftCard FROM TB_Order AS qq
				LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard
				WHERE qq.codeGitfCard = @pCodeGitfCard) 


				 IF EXISTS (SELECT * FROM TB_GiftCardUsedForPurchase WHERE codeGiftCard = @pCodeGitfCard) 
					
				 BEGIN
				  SELECT gi.idGiftCard,gi.[value],gi.DateExpiration,idStatusGiftCard,(SELECT st.nameStatus FROM TB_StatusGiftCard AS st WHERE st.idStatusGiftCard = gi.idStatusGiftCard) AS nameStatus
				,(SELECT balance FROM TB_GiftCardUsedForPurchase WHERE codeGiftCard = @pCodeGitfCard) AS balance 
				FROM TB_Order AS qq
			    LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard

			    WHERE qq.codeGitfCard = @pCodeGitfCard
				   END
					else
					 BEGIN 

			      SELECT gi.idGiftCard,gi.[value],gi.DateExpiration,idStatusGiftCard,(SELECT st.nameStatus FROM TB_StatusGiftCard AS st WHERE st.idStatusGiftCard = gi.idStatusGiftCard) AS nameStatus 
				  ,CONVERT(INT, gi.[value]) AS balance
				  FROM TB_Order AS qq
				  LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard
				  WHERE qq.codeGitfCard = @pCodeGitfCard

				
				END

				  --SELECT * FROM TB_Order 
				  --WHERE codeGitfCard = 'SGNFAL0NEW'
				  --SELECT * FROM TB_GiftCard 




			    SELECT gi.idGiftCard FROM TB_Order AS qq
				LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCardUsedForPurchase
				WHERE qq.codeGitfCard = @pCodeGitfCard


end





GO
