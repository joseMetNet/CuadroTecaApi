/****** Object:  StoredProcedure [dbo].[SP_getPromotion]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_getPromotion]
@pidPromotion  int = null
AS
BEGIN


					SELECT qq.[idPromotion]
						  ,[idPromotionType]
						  ,(SELECT NamePromotionType FROM TB_PromotionType AS pr WHERE pr.idPromotionType = qq.idPromotionType) AS NamePromotionType
						  ,[idProduct]
						  ,(SELECT NameProduct FROM TB_Product AS duc WHERE duc.idProduct = qq.idProduct) AS NameProduct
						  ,[idCategory]
						  ,(SELECT NameCategory FROM TB_Category AS ct WHERE ct.idCategory = qq.idCategory) AS NameCategory
						  ,[NamePromotion]
						  ,[descriptionPromotion]
						  ,[promotionCode]
						  ,[condition]
						  ,[couponAmount]
						  ,[discountValue]
						  ,CONVERT(VARCHAR(10),startDate,110) AS startDate
						  ,CONVERT(VARCHAR(10),endDate,110) AS endDate
						  ,[idStatusPromotion]
						  ,(SELECT nameStatus FROM TB_StatusPromotion AS sp WHERE sp.idStatusPromotion = qq.idStatusPromotion) AS nameStatus
						  ,CONVERT(VARCHAR(10),qq.[dateAdd],110) AS dateAdd
						  ,cy.[idStore]
						  ,(SELECT country FROM TB_Store AS st WHERE st.idStore = qq.idStore) country
						  ,ci.idCity
						  ,Cy.City
				  FROM [dbo].[TB_promotion] AS qq
				  LEFT JOIN [dbo].[TB_CitiesPromotion] AS ci ON ci.idPromotion = qq.idPromotion
				  LEFT JOIN TB_City AS Cy ON Cy.idCity = ci.idCity
			      where @pidPromotion is null or qq.idPromotion = @pidPromotion
			      --where qq.idProduct =  14

				  --SELECT * FROM TB_City
				  --SELECT * FROM TB_PromotionType
				  --SELECT * FROM TB_Promotion
				  --SELECT * FROM TB_Product
				  --SELECT * FROM TB_Category
				  --SELECT * FROM TB_StatusPromotion
				  --SELECT * FROM TB_Store
				  --SELECT * FROM [TB_CitiesPromotion]




end



GO
/****** Object:  StoredProcedure [dbo].[SP_getPromotionByCode]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_getPromotionByCode]
@PromotionCode varchar(max) = null
AS
BEGIN


					SELECT qq.[idPromotion]
						  ,[idPromotionType]
						  ,(SELECT NamePromotionType FROM TB_PromotionType AS pr WHERE pr.idPromotionType = qq.idPromotionType) AS NamePromotionType
						  ,[idProduct]
						  ,(SELECT NameProduct FROM TB_Product AS duc WHERE duc.idProduct = qq.idProduct) AS NameProduct
						  ,[idCategory]
						  ,(SELECT NameCategory FROM TB_Category AS ct WHERE ct.idCategory = qq.idCategory) AS NameCategory
						  ,[NamePromotion]
						  ,[descriptionPromotion]
						  ,[promotionCode]
						  ,[condition]
						  ,[couponAmount]
						  ,[discountValue]
						  ,CONVERT(VARCHAR(10),startDate,110) AS startDate
						  ,CONVERT(VARCHAR(10),endDate,110) AS endDate
						  ,[idStatusPromotion]
						  ,(SELECT nameStatus FROM TB_StatusPromotion AS sp WHERE sp.idStatusPromotion = qq.idStatusPromotion) AS nameStatus
						  ,CONVERT(VARCHAR(10),qq.[dateAdd],110) AS dateAdd
						  ,cy.[idStore]
						  ,(SELECT country FROM TB_Store AS st WHERE st.idStore = qq.idStore) country
						  ,ci.idCity
						  ,Cy.City
				  FROM [dbo].[TB_promotion] AS qq
				  LEFT JOIN [dbo].[TB_CitiesPromotion] AS ci ON ci.idPromotion = qq.idPromotion
				  LEFT JOIN TB_City AS Cy ON Cy.idCity = ci.idCity

			      where promotionCode = @PromotionCode
			      --where qq.idProduct =  14

				  --SELECT * FROM TB_City
				  --SELECT * FROM TB_PromotionType
				  --SELECT * FROM TB_Promotion
				  --SELECT * FROM TB_Product
				  --SELECT * FROM TB_Category
				  --SELECT * FROM TB_StatusPromotion
				  --SELECT * FROM TB_Store
				  --SELECT * FROM [TB_CitiesPromotion]




end



GO
