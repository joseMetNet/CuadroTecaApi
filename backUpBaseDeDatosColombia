-- =============================================
CREATE PROCEDURE [dbo].[SP_StoreOrderGiftCard]

@pidShippingUser int = null,
@pId varchar(max) = '',
@pidcurrency varchar(max) = '',
@pcountry varchar(max) = '',
@idPromotion int = null,
@pidGiftCard int = null,
@pTotalPrice varchar(max) = '',
@pCodeGitfCard varchar(max) = ''




AS
BEGIN
declare @intos0 table (id int);
declare @vidOrder int;



						INSERT INTO [dbo].[TB_Order]
								   ([dateAdd],
								    [idStatusOrder],
								    [idShippingUser],
								    [IdMercadoPago],
									[idStatusPayOrder],
									[idPromotion],
									[StatusEmailSale],
									[StatusEmailShipping],
									[StatusEmailDelivery],
									[StatusPoll],
									[invoice],
									[surveyResult],
									[country],
									[TotalPrice],
									[codeGitfCard],
									[idGiftCard])
								   output inserted.idOrder into @intos0
							 VALUES
								   ((dateadd(hour,(-5),getdate())),
								   2,@pidShippingUser,@pId,0,@idPromotion,'pendiente','pendiente','pendiente','pendiente','pendiente','pendiente',@pcountry,@pTotalPrice,@pCodeGitfCard,@pidGiftCard)

								   UPDATE TB_promotion SET couponAmount = couponAmount - 1
								   WHERE idPromotion = @idPromotion
								  --SELECT * FROM [dbo].[TB_StatusOrder]
								   set @vidOrder = (select id from @intos0);

								 --SELECT * FROM TB_Order
								 --SELECT * FROM TB_HistoOrder

								 
					   --UPDATE TB_ShippingUser SET present = 'true'
					   --WHERE idShippingUser = @pidShippingUser
								 
						INSERT INTO [dbo].[TB_HistoOrderGiftCard]
								   ([idOrder],
								    [CodeGiftCard])
								
							 VALUES
								   (@vidOrder,@pCodeGitfCard)

						
				
END
GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateGiftCard]
@pidGiftCard int = 0,
@pNameGiftCard varchar(max) = '',
@pReference varchar(max) = '',
@pValue decimal(18,2) = 0,
@pIVA int = null



AS
BEGIN
declare @intos table (id int);
declare @vidGiftCard int;

    if(@pidGiftCard > 0)
	begin
			UPDATE [dbo].[TB_GiftCard]
			   SET 
			       [NameGiftCard] = isnull(@pNameGiftCard,[NameGiftCard])
				  ,[reference] = isnull(@pReference,[reference])
				  ,[value] = isnull(@pValue,[value])
				  ,[IVA] = isnull(@pIVA,[IVA])

			 WHERE idGiftCard = @pidGiftCard

			 set @vidGiftCard = @pidGiftCard;
			 select 
		  
		  'Se Actualizo una GiftCard' AS api_Dice
		   ,idGiftCard
		   ,NameGiftCard
		   ,idStatusGiftCard
		   ,idStatusGiftCard
		   ,(SELECT sg.nameStatus FROM TB_StatusGiftCard AS sg WHERE sg.idStatusGiftCard = qq.idStatusGiftCard ) nameStatus
		   ,isnull(IVA,0) AS IVA

		from [TB_GiftCard] AS qq where idGiftCard = @vidGiftCard;
	end
	else
	begin
		
			INSERT INTO [dbo].[TB_GiftCard]
				   ([NameGiftCard]
				   ,[reference]
				   ,[value]
				   ,[idStatusGiftCard]
				   ,[DateExpiration]
				   ,[IVA])
				output inserted.idGiftCard into @intos
			 VALUES
				   (@pNameGiftCard
				   ,@pReference
				   ,@pValue
				   ,2
				   ,DATEADD(year, 1, (dateadd(hour,(-5),getdate())))
				   ,@pIVA)
				   
			 set @vidGiftCard = (select id from @intos);

			 
	end

	   select 
		  
		  'Se creo una nueva GiftCard' AS api_Dice
		   ,idGiftCard
		   ,NameGiftCard
		   ,idStatusGiftCard
		   ,isnull(IVA,0) AS IVA
		   ,(SELECT sg.nameStatus FROM TB_StatusGiftCard AS sg WHERE sg.idStatusGiftCard = qq.idStatusGiftCard ) nameStatus

		from [TB_GiftCard] AS qq where idGiftCard = @vidGiftCard;
	

		/*
		SELECT * FROM [TB_GiftCard]
		SELECT * FROM [TB_GiftCard]
		*/


END

GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdatePromotion]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdatePromotion]
@pidPromotion int = 0,
@pidPromotionType int = 0,
@pidProduct int = 0,
@pidCategory int = 0,
@pDescriptionPromotion varchar(max) = '',
@pNamePromotion varchar(max) = '',
@pPromotionCode varchar(max) = '',
@pCondition varchar(max) = '',
@pCouponAmount int = 0,
@pDiscountValue int = 0,
@pStartDate datetime = '', 
@pEndDate datetime = '',
@pNameCitysPromotion varchar(max) = '',
@pNameStoresPromotion varchar(max) = '',
@pidStore int = 0

AS
BEGIN
declare @intos table (id int);
declare @vidPromotion int;
declare @vdateMin datetime ;

--declare @vidCity int;


			--declare @d table (id int, name varchar(max));
			--insert @d EXEC	[dbo].SP_storeCities @pNameCities = @pNameCity
			--set @vidCity = (select id from @d)
		


       if(@pidPromotion > 0)
		begin
			UPDATE [dbo].[TB_promotion]
			  SET 
				   [idPromotionType] = isnull(@pidPromotionType,[idPromotionType])
			      ,[idProduct] = isnull(@pidProduct,[idProduct])
				  ,[idCategory] = isnull(@pidCategory,[idCategory])
				  ,[NamePromotion] = isnull(@pNamePromotion,[NamePromotion])
				  ,[descriptionPromotion] = isnull(@pDescriptionPromotion,[descriptionPromotion])
				  ,[promotionCode] = isnull(@pPromotionCode,[promotionCode])
				  ,[condition] = isnull(@pCondition,[condition])
				  ,[couponAmount] = isnull(@pCouponAmount,[couponAmount])
				  ,[discountValue] = isnull(@pDiscountValue,[discountValue])
				  ,[startDate] = isnull(@pStartDate,[startDate])
				  ,[endDate] = isnull(@pEndDate,[endDate])


			 WHERE idPromotion = @pidPromotion

			 set @vidPromotion = @pidPromotion;

			   insert into [TB_CitiesPromotion]
				  ([idPromotion]
				  ,[idCity])

                select [value] = @vidPromotion , [value]
				from string_split(@pNameCitysPromotion,',')

								  SET @vdateMin = ( SELECT MIN(dateAdd) FROM [TB_CitiesPromotion]

											 WHERE idPromotion = @pidPromotion)

											 DELETE FROM [TB_CitiesPromotion]

											 WHERE dateAdd = @vdateMin
	end
	else
	begin
		
			INSERT INTO [dbo].[TB_promotion]
				   (
				    [idPromotionType]
				   ,[idProduct]
				   ,[idCategory]
				   ,[descriptionPromotion]
				   ,[NamePromotion]
				   ,[promotionCode]
				   ,[condition]
				   ,[couponAmount]
				   ,[discountValue]
				   ,[startDate]
				   ,[endDate]
				   ,[idStatusPromotion]
				   ,[idStore]
				   )
				output inserted.idPromotion into @intos
			 VALUES
				   (@pidPromotionType
				   ,@pidProduct
				   ,@pidCategory
				   ,@pDescriptionPromotion
				   ,@pNamePromotion
				   ,isnull(@pPromotionCode,'')
				   ,@pCondition
				   ,isnull(@pCouponAmount,0)
				   ,@pDiscountValue
				   ,@pStartDate
				   ,@pEndDate
				   ,1
				   ,@pidStore
				   )
				   
			 set @vidPromotion = (select id from @intos);

			 insert into [TB_CitiesPromotion]
				  ([idPromotion]
				  ,[idCity])

                select [value] = @vidPromotion , [value]
				from string_split(@pNameCitysPromotion,',')

	end

	select 
			* 
		from [dbo].[TB_promotion] where idPromotion = @vidPromotion;


		--SELECT * FROM [TB_CitiesPromotion]
		--SELECT * FROM TB_City
	


END
GO
/****** Object:  StoredProcedure [dbo].[SP_storeOrUpdateShippingAddress]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
