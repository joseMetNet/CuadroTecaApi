/****** Object:  StoredProcedure [dbo].[SP_GetCountry]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[SP_GetCountry]
@pidCountry int = 0

AS
BEGIN

IF (@pidCountry > 0)
BEGIN

SELECT idCountry,country,shippingPrice,idTypeOfcurrency,(SELECT currency FROM [dbo].[TB_TypeOfcurrency] AS ee WHERE qq.idTypeOfcurrency = ee.idTypeOfcurrency) AS currency,[dateAdd],squarePrice

FROM [dbo].[TB_Country] AS qq

WHERE idCountry = @pidCountry

END

ELSE

BEGIN

SELECT idCountry,country,shippingPrice,idTypeOfcurrency,(SELECT currency FROM [dbo].[TB_TypeOfcurrency] AS ee WHERE qq.idTypeOfcurrency = ee.idTypeOfcurrency) AS currency,[dateAdd],squarePrice

FROM [dbo].[TB_Country] AS qq

END 

END
GO
/****** Object:  StoredProcedure [dbo].[SP_GetDataEmailAddressUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetDataEmailAddressUser]
@pid VARCHAR(MAX) = '',
@pidShippingUser INT = NULL,
@pidOrder INT = NULL

AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

   
         IF(@pidOrder > 0)
		 BEGIN
		 
		 SELECT 
	     qq.[Name] + ' ' + qq.lastName AS FullName,
		 qq.email,
		 qq.[address],
		 qq.Phone,
		 qq.idCity,
		 qq.idShippingAddress,
		 (SELECT cy.[City] FROM TB_City AS cy WHERE cy.idCity = qq.idCity) AS City
		 FROM TB_ShippingAddress AS qq
		 LEFT JOIN TB_ShippingUser AS us ON us.idShippingUser = qq.idShippingUser
		 LEFT JOIN TB_Order AS ord ON ord.idShippingUser = ord.idShippingUser
		 WHERE qq.idShippingUser = @pidShippingUser
		 AND ord.idOrder = @pidOrder
		 END 
		 ELSE
		 BEGIN
		 SELECT 
	     qq.[Name] + ' ' + qq.lastName AS FullName,
		 qq.email
		 FROM TB_ShippingAddress AS qq
		 LEFT JOIN TB_ShippingUser AS us ON us.idShippingUser = qq.idShippingUser
		 LEFT JOIN TB_Order AS ord ON ord.idShippingUser = ord.idShippingUser
		 WHERE ord.IdMercadoPago = @pid
		 AND qq.idShippingUser = @pidShippingUser
		 END

		 --SELECT * FROM TB_ShippingAddress
		 --SELECT * FROM TB_ShippingUser
		 --SELECT * FROM [dbo].[TB_StatusOrder]
		 --SELECT * FROM TB_Order 


		

		end
	
	    




GO
/****** Object:  StoredProcedure [dbo].[SP_GetDataEmailShippingUser]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetDataEmailShippingUser]
@pid VARCHAR(MAX) = '',
@pidOrder int = 0

AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

   
         IF(@pidOrder > 0)
		 BEGIN
		 

		 	 SELECT 
			 sh.idShippingUser,
			 qq.idOrder,
			 sh.Name + ' ' + sh.lastName AS Fullname,
			 sh.email,
			 st.Name AS NameStatus,
			 sh.present,
			 isnull(adr.[address],sh.[address]) AS address
			,sh.phone
			,isnull(adr.idCity,sh.idCity) AS idCity
			,isnull((SELECT cy.City FROM TB_City AS cy WHERE cy.idCity = adr.idCity),(SELECT cy.City FROM TB_City AS cy WHERE cy.idCity = sh.idCity)) NameCity
			,qq.guide
			,qq.conveyor
			 
			 FROM  TB_Order AS qq
			 LEFT JOIN TB_ShippingUser AS sh ON sh.idShippingUser = qq.idShippingUser
			 LEFT JOIN TB_StatusOrder AS st ON st.idStatusOrder = qq.idStatusOrder
			 LEFT JOIN TB_ShippingAddress AS adr ON adr.idShippingUser = sh.idShippingUser
			 
			 WHERE qq.idOrder = @pidOrder


		 END 
		 ELSE 
		 BEGIN

			 SELECT 
			 sh.idShippingUser,
			 qq.idOrder,
			 sh.Name + ' ' + sh.lastName AS Fullname,
			 sh.email,
			 st.Name AS NameStatus,
			 sh.present,
			 qq.guide,
			 qq.conveyor
			 FROM  TB_Order AS qq
			 LEFT JOIN TB_ShippingUser AS sh ON sh.idShippingUser = qq.idShippingUser
			 LEFT JOIN TB_StatusOrder AS st ON st.idStatusOrder = qq.idStatusOrder
			 WHERE qq.IdMercadoPago = @pid

		 END 

		 
         

		 --SELECT * FROM TB_ShippingAddress
		 --SELECT * FROM TB_ShippingUser
		 --SELECT * FROM [dbo].[TB_StatusOrder]
		 --SELECT * FROM TB_Order 
		END
	
	    




GO
