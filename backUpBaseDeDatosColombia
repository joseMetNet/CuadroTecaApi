		    from TB_Users where idUser = @pidUser
			
	end
	else
	begin
		
			select 
		     [idUser]
			,[firstName]
			,[lastName]
			,[idTypeUser]
			,[email]
			--,[userName]
			,idStatusUser idStatus 
		    from TB_Users

	end

	
		/*
		SELECT * FROM TB_Users
		*/

END

