-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_GetProduct]
@pidProduct int = 0
AS
BEGIN
declare @intos table (id int);
declare @vIdUser int;

    if(@pidProduct > 0)
	begin
		
	
		

			SELECT [idProduct]
				  ,[NameProduct]
				  ,[dateAdd]
			  FROM [dbo].[TB_Product] AS qq
			  WHERE idProduct = @pidProduct




			
	end
	else
	begin
		
			SELECT [idProduct]
				  ,[NameProduct]
				  ,[dateAdd]
			  FROM [dbo].[TB_Product] AS qq

	end

	
		/*
		SELECT * FROM TB_Users
		*/

END

