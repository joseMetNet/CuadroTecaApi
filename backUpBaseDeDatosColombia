-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_storeOrUpdateVisitsWeb]
@pidVisitsWeb int = 0,
@pCity varchar(max) = null,
@pPossiblePurchase varchar(max) = null


AS
BEGIN
declare @intos table (id int);
declare @vidVisitsWeb int ;



	

       if(@pidVisitsWeb > 0)
		begin
			UPDATE [dbo].[TB_VisitsWeb]
			  SET  [possiblePurchase] = isnull(@pPossiblePurchase,[possiblePurchase] )

			 WHERE idVisitsWeb = @pidVisitsWeb

			 set @vidVisitsWeb = @pidVisitsWeb;

				select  *
						from [TB_VisitsWeb] AS qq where idVisitsWeb = @vidVisitsWeb;
		
	end
	else
	begin
		



		INSERT INTO [dbo].[TB_VisitsWeb]
				   ([possiblePurchase]
				   ,[City])

			output inserted.idVisitsWeb into @intos
			 VALUES
				   ('no'
				   ,@pCity)

	      set @vidVisitsWeb = (select id from @intos);

	end

					select  *
						from [TB_VisitsWeb] AS qq where idVisitsWeb = @vidVisitsWeb;

END
GO
