/****** Object:  StoredProcedure [dbo].[SP_	GetPendingToUseGiftCard]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_	GetPendingToUseGiftCard]








AS
BEGIN
     

		 select ads.email,ads.[Name] + ' '  + ads.lastName AS nombreResive,shi.[Name] + ' ' + shi.lastName AS nombreComprador,qq.codeGitfCard from TB_Order AS qq
		 LEFT JOIN TB_GiftCard AS gi ON gi.idGiftCard = qq.idGiftCard
		 LEFT JOIN TB_ShippingUser AS shi ON shi.idShippingUser = qq.idShippingUser
		 LEFT JOIN TB_ShippingAddress AS ads ON ads.idShippingUser = shi.idShippingUser
		 WHERE gi.DateExpiration < (dateadd(hour,(-5),getdate()))
		 --select * from TB_Order WHERE [dateAdd] <= DATEADD(DAY, -1, GETDATE())
		 --select * from TB_GiftCard AS qq

		 --UPDATE TB_GiftCard SET DateExpiration = '2021-01-12'
		 --WHERE idGiftCard = 52


END
