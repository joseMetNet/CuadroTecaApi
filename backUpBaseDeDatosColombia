/****** Object:  StoredProcedure [dbo].[SP_UpdateStatusOrder]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


-- =============================================
-- Author:      <Author, , Name>
-- Create Date: <Create Date, , >
-- Description: <Description, , >
-- =============================================
CREATE PROCEDURE [dbo].[SP_UpdateStatusOrder]
@Status int = null,
@pIdMercadoPago varchar(max) = null ,
@pIdOrder int = 0,
@pBalance int = null,
@pCode varchar(max) = null

AS           
BEGIN        
DECLARE @pvCodeGiftCardUsedForPurchase varchar(max) = null



        IF(@pIdOrder > 0)
		BEGIN
			UPDATE TB_Order
			SET idStatusOrder = 1,
			idStatusPayOrder = @Status
			WHERE idOrder = @pIdOrder
		
		    SELECT isnull(qq.idGiftCard,0) AS idGiftCard ,isnull(qq.codeGitfCard,'la compra no es una gitf card') AS codeGitfCard FROM TB_Order qq
			WHERE IdMercadoPago = @pIdMercadoPago

			SET @pvCodeGiftCardUsedForPurchase  = (SELECT idGiftCardUsedForPurchase FROM TB_Order WHERE idOrder = @pIdOrder)
            
			IF(@Status = 3)
			BEGIN
			--IF EXISTS (SELECT * FROM TB_GiftCardUsedForPurchase WHERE idGiftCardUsedForPurchase = @pvid) 
			   --BEGIN
					   UPDATE [TB_GiftCardUsedForPurchase] SET balance = @pBalance
					   WHERE codeGiftCard = @pCode
					-- END
					--else
					-- BEGIN
					--	 INSERT INTO [dbo].[TB_GiftCardUsedForPurchase]
					--	([idGiftCardUsedForPurchase],
					--	 [balance])
					--    VALUES
					--	  (@pidGiftCardUsedForPurchase
					--	  ,@pBalance)

					--	 UPDATE TB_Order SET UsedGiftCard = 'SI'
					--	 WHERE idOrder = @pIdMercadoPago 
					-- END



			 END
			 END
			 ELSE
			 BEGIN
				UPDATE TB_Order
				SET idStatusOrder = 1,
				idStatusPayOrder = @Status
				WHERE IdMercadoPago = @pIdMercadoPago
		
				SELECT isnull(qq.idGiftCard,0) AS idGiftCard ,isnull(qq.codeGitfCard,'la compra no es una gitf card') AS codeGitfCard FROM TB_Order qq
				WHERE IdMercadoPago = @pIdMercadoPago

				SET @pvcodeGiftCardUsedForPurchase  = (SELECT codeGiftCardUsedForPurchase FROM TB_Order WHERE IdMercadoPago = @pIdMercadoPago)
            
			IF(@Status = 3)
			BEGIN
			--IF EXISTS (SELECT * FROM TB_GiftCardUsedForPurchase WHERE idGiftCardUsedForPurchase = @pvid) 
					
			--		BEGIN
					   UPDATE [TB_GiftCardUsedForPurchase] SET balance = balanceTemporary
					   WHERE codeGiftCard = @pvCodeGiftCardUsedForPurchase
					      
						 
					-- END
					--else
					-- BEGIN
					--	 INSERT INTO [dbo].[TB_GiftCardUsedForPurchase]
					--	([idGiftCardUsedForPurchase],
					--	 [balance])
					--    VALUES
					--	  (@pidGiftCardUsedForPurchase
					--	  ,@pBalance)

					--	 UPDATE TB_Order SET UsedGiftCard = 'SI'
					--	 WHERE idOrder = @pIdMercadoPago 
					-- END
			 END
			 END
				--SELECT * FROM TB_StatusOrder
				--SELECT * FROM [dbo].[TB_StatusPayOrder]
				--SELECT * FROM TB_Order 
			    --SELECT * FROM TB_ShippingUser
				--SELECT * FROM TB_GiftCard
				--SELECT * FROM TB_StatusSaleGiftCard
				--SELECT * FROM TB_GiftCardUsedForPurchase
				--DELETE  FROM TB_GiftCardUsedForPurchase

END
GO
/****** Object:  StoredProcedure [dbo].[SP_UpdateStatusPromotion]    Script Date: 27/01/2023 8:53:12 a. m. ******/
