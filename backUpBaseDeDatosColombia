/****** Object:  StoredProcedure [dbo].[ObtenerInfoValorConfiguracion]    Script Date: 27/01/2023 8:53:12 a. m. ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


		create PROCEDURE [dbo].[ObtenerInfoValorConfiguracion] @Nombre VARCHAR(50)=NULL
AS  
BEGIN  
	SET NOCOUNT ON;
	
	SELECT cv.idValor, cv.Nombre, cv.Valor 
	FROM ValorConfiguracion cv WITH(NOLOCK) 
	WHERE Estado=1 AND Nombre = @Nombre
	
END
